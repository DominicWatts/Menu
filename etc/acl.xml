				<resource id="Xigen_Menu::Menu" sortOrder="10" title="Menu">
					<resource id="Xigen_Menu::Menu_save" sortOrder="10" title="Save Menu"/>
					<resource id="Xigen_Menu::Menu_delete" sortOrder="20" title="Delete Menu"/>
					<resource id="Xigen_Menu::Menu_update" sortOrder="30" title="Update Menu"/>
					<resource id="Xigen_Menu::Menu_view" sortOrder="40" title="View Menu"/>
