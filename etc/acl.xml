<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Xigen_Menu::Item" sortOrder="10" title="Item">
					<resource id="Xigen_Menu::Item_save" sortOrder="10" title="Save Item"/>
					<resource id="Xigen_Menu::Item_delete" sortOrder="20" title="Delete Item"/>
					<resource id="Xigen_Menu::Item_update" sortOrder="30" title="Update Item"/>
